<script type="text/javascript">


	
	$("#menuOpcoes").mouseover(function(){
		if(btnAnimacao){
			$("#menuOpcoes").removeClass("menuSide").addClass("menuSideClone");
			return;
		}
		 
		$("i").removeClass('.logosMenu').addClass('logosMenuExtend');
		$(".textoBtn").css("visibility", "visible");
		$(".ctrltext").removeClass("textspn").addClass("textspnShow");
	}).mouseout(function(){
		if(btnAnimacao){
			$("#menuOpcoes").removeClass("menuSideClone").addClass("menuSide");
			return;
		}
		$("i").removeClass('logosMenuExtend').addClass('.logosMenu');
		$(".textoBtn").css("visibility", "hidden");
		$(".ctrltext").removeClass("textspnShow").addClass("textspn");
		// $('body').css("background-image", "url('dudley.jpg')");
	});

